{
    "name": "file-structure",
    "displayName": "file-structure",
    "description": "模仿 IntelliJ IDEA 的 file-structure 功能实现的 vscode 插件",
    "version": "0.0.1",
    "license": "MIT",
    "publisher": "dailer",
    "engines": {
        "vscode": "^1.14.0"
    },
    "keywords": [
        "structure"
    ],
    "categories": [
        "Other"
    ],
    "icon": "./media/icons/logo.ico",
    "activationEvents": [
        "onView:fileStructure",
        "onLanguage:javascript",
        "*"
    ],
    "main": "./src/extension",
    "contributes": {
        "commands": [{
            "command": "fileStructure.refreshEntry",
            "title": "Structure",
            "icon": {
                "light": "resources/light/refresh.svg",
                "dark": "resources/dark/refresh.svg"
            }
        }],
        "views": {
            "explorer": [{
                "id": "fileStructure",
                "name": "structure"
            }]
        },
        "menus": {
            "view/title": [{
                "command": "fileStructure.refreshEntry",
                "when": "view == fileStructure",
                "group": "navigation"
            }],
            "explorer/context": [{
                "command": "fileStructure.refreshEntry"
            }],
            "editor/title": [{
                "command": "fileStructure.refreshEntry",
                "alt": "markdown.showPreviewToSide",
                "group": "navigation"
            }]
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32",
        "jsonc-parser": "^0.4.2"
    },
    "dependencies": {
        "jsonc-parser": "^0.4.2"
    }
}